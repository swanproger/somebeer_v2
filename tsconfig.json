{
    "compilerOptions": {
      "jsx": "react-jsx",
      "allowJs": true, 
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "typeRoots": ["./node_modules/@types", "./src/types"]
    },
    "include": ["src/**/*", "src/assets/**/*"],
    "exclude": ["node_modules"]
  }